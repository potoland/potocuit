{
	"name": "@potoland/framework",
	"packemon": [
		{
			"inputs": {
				"index": "src/index.ts"
			},
			"platform": "node",
			"format": [
				"lib",
				"mjs"
			]
		}
	],
	"version": "0.0.5",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/**",
		"src/**/*"
	],
	"scripts": {
		"build": "tsup",
		"clean": "rm -rf dist && rm -rf .turbo",
		"dev": "tsup --watch",
		"prepublishOnly": "npm run build"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"require": "./dist/index.js"
		}
	},
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.68",
		"@types/node": "^20.1.3",
		"packemon": "^3.2.0"
	},
	"license": "Apache-2.0",
	"author": "MARCROCK22",
	"contributors": [
		{
			"name": "MARCROCK22",
			"url": "https://github.com/MARCROCK22",
			"author": true
		},
		{
			"name": "socram03",
			"url": "https://github.com/socram03",
			"author": true
		}
	],
	"homepage": "https://biscuitjs.com",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/potoland/potocuit.git"
	},
	"bugs": {
		"url": "https://github.com/potoland/potocuit"
	},
	"keywords": [
		"api",
		"discord",
		"bots",
		"typescript",
		"botdev"
	],
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@biscuitland/common": "^0.0.8",
		"@biscuitland/helpers": "^3.0.4",
		"@biscuitland/ws": "^3.0.8",
		"@potoland/structures": "^0.0.5",
		"tsup": "^7.1.0"
	},
	"engines": {
		"node": ">=16.12.0"
	}
}