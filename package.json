{
    "name": "@potoland/core",
    "version": "0.0.1",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsc --outDir ./dist",
        "dev": "swc src -d dist -w",
        "prepublishOnly": "npm run build",
        "prepare": "npm run build && husky install",
        "lint": "biome lint --apply ./src",
        "format": "biome format --write ./src",
        "check": "biome check --apply ./src",
        "test:run": "cd __test__ && node ./dist/index.js",
        "test:build": "cd __test__ && nodemon --ext ts --watch ./src --exec \"swc ./src --config-file ../.swcrc -d ./dist\"",
        "test": "cd __test__ && nodemon --ext ts --watch ./src --exec \"swc ./src --config-file ../.swcrc -d ./dist && node ./dist/index.js\""
    },
    "author": "MARCROCK22",
    "license": "ISC",
    "dependencies": {
        "@biscuitland/common": "^0.0.9",
        "@biscuitland/rest": "^3.0.7",
        "form-data": "^4.0.0",
        "ioredis": "^5.3.2",
        "ts-mixer": "^6.0.3",
        "ws": "^8.15.1"
    },
    "devDependencies": {
        "@biomejs/biome": "1.4.1",
        "@swc/cli": "^0.1.62",
        "@swc/core": "^1.3.93",
        "@types/node": "^20.6.3",
        "@types/ws": "^8.5.10",
        "nodemon": "^3.0.1",
        "typescript": "^5.2.2",
        "husky": "^8.0.0"
    },
    "optionalDependencies": {
        "tweetnacl": "^1.0.3",
        "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.32.0"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.mjs"
            },
            "require": "./dist/index.js"
        }
    },
    "homepage": "https://biscuitjs.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/potoland/potocuit.git"
    },
    "bugs": {
        "url": "https://github.com/potoland/potocuit"
    },
    "keywords": [
        "api",
        "discord",
        "bots",
        "typescript",
        "botdev"
    ],
    "publishConfig": {
        "access": "public"
    }
}
