{
    "sourceMaps": false,
    "module": {
        "type": "commonjs",
        "strictMode": true,
        "noInterop": false
    },
    "jsc": {
        "externalHelpers": false,
        "target": "esnext",
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true,
            "react": {
                "throwIfNamespace": false,
                "useBuiltins": false,
                "pragma": "React.createElement",
                "pragmaFrag": "React.Fragment",
                "importSource": "react"
            }
        },
        "keepClassNames": true
    }
}
